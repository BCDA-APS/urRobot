epicsEnvSet("EPICS_BASE","/APSshare/epics/base-7.0.8")
epicsEnvSet("PREFIX", "ur621:")
dbLoadDatabase("../../dbd/iocur621Linux.dbd")
iocur621Linux_registerRecordDeviceDriver(pdbbase)
< urRobot.cmd
