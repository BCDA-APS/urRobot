# Substitutions file to load GIXS samples
#
# Each sample (block) is given an ID and string comment for metadata

file "gixs_sample.db" {
    pattern
    {BlockID, COMMENT}
    {1, "This is sample ID: $(BlockID)"}
    {2, "This is sample ID: $(BlockID)"}
    {3, "This is sample ID: $(BlockID)"}
    {4, "This is sample ID: $(BlockID)"}
    {5, "This is sample ID: $(BlockID)"}
}
