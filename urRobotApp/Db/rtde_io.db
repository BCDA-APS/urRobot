record(ao, "$(P)$(R)RTDE:SpeedSlider")
{
    field(DESC, "Speed slider value(percentage)")
    field(DTYP, "asynFloat64")
    field(DRVH, "1.0")
    field(DRVL, "0.0")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))SPEED_SLIDER")
}

record(bo, "$(P)$(R)RTDE:SetStandardDOUT0")
{
    field(DESC, "Standard digital output 0")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))SET_STANDARD_DIGITAL_OUT0")
}

