# This database defines a single waypoint for the robot in joint space
# including the gripper state where 0=open and 1=closed

record(stringout, "$(P)$(R=)WaypointJ:$(N):Name")
{
    field(VAL, "$(DESC=)")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J1")
{
    field(VAL, "$(J1=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J2")
{
    field(VAL, "$(J2=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J3")
{
    field(VAL, "$(J3=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J4")
{
    field(VAL, "$(J4=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J5")
{
    field(VAL, "$(J5=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):J6")
{
    field(VAL, "$(J6=0)")
    field(PREC, 2)
    field(EGU, "deg")
}

record(bo, "$(P)$(R=)WaypointJ:$(N):Gripper")
{
    field(ZNAM, "OPEN")
    field(ONAM, "CLOSED")
    field(VAL, "$(GRIPPER)")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):Speed")
{
    field(VAL, "$(SPEED=1.05)")
    field(PREC, 2)
    field(EGU, "rad/s")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):Acceleration")
{
    field(VAL, "$(ACCEL=1.4)")
    field(PREC, 2)
    field(EGU, "rad/s/s")
}

record(ao, "$(P)$(R=)WaypointJ:$(N):Blend")
{
    field(VAL, "$(BLEND=0.0)")
    field(PREC, 2)
    field(EGU, "m")
}

# 1 if robot is at waypoint within tolerance, otherwise 0
record(luascript, "$(P)$(R=)WaypointJ:$(N):Reached") {
    field(INPA, "$(P)$(R=)Receive:ActualJointPositions0 CP")
    field(INPB, "$(P)$(R=)Receive:ActualJointPositions1 CP")
    field(INPC, "$(P)$(R=)Receive:ActualJointPositions2 CP")
    field(INPD, "$(P)$(R=)Receive:ActualJointPositions3 CP")
    field(INPE, "$(P)$(R=)Receive:ActualJointPositions4 CP")
    field(INPF, "$(P)$(R=)Receive:ActualJointPositions5 CP")
    field(CODE, "@waypoints.lua waypointJ_reached({prefix='$(P)$(R=)', N=$(N), tol=$(TOL=1e-2)})")
    field(PREC, 0)
    field(VAL, 0)
}

# stores DOLx.VAL in LNKx.VAL
# Requires that RTDE Receive databases have been loaded
record(seq, "$(P)$(R=)WaypointJ:$(N):Reset")
{
    # Sets waypoint joint angles to the current measured angles
    field(DOL0, "$(P)$(R=)Receive:ActualJointPositions0")
    field(LNK0, "$(P)WaypointJ:$(N):J1 PP")
    field(DOL1, "$(P)$(R=)Receive:ActualJointPositions1")
    field(LNK1, "$(P)WaypointJ:$(N):J2 PP")
    field(DOL2, "$(P)$(R=)Receive:ActualJointPositions2")
    field(LNK2, "$(P)WaypointJ:$(N):J3 PP")
    field(DOL3, "$(P)$(R=)Receive:ActualJointPositions3")
    field(LNK3, "$(P)WaypointJ:$(N):J4 PP")
    field(DOL4, "$(P)$(R=)Receive:ActualJointPositions4")
    field(LNK4, "$(P)WaypointJ:$(N):J5 PP")
    field(DOL5, "$(P)$(R=)Receive:ActualJointPositions5")
    field(LNK5, "$(P)WaypointJ:$(N):J6 PP")
}

record(bo, "$(P)$(R=)WaypointJ:$(N):moveJ")
{
    field(FLNK, "$(P)$(R=)WaypointJ:$(N):moveJ_seq1.PROC")
}

record(seq, "$(P)$(R=)WaypointJ:$(N):moveJ_seq1")
{
    # Disable AutoMoveJ
    field(DOL0, "0")
    field(LNK0, "$(P)$(R=)Control:AutoMoveJ PP")

    # Set Control:JxCmd to WaypointJ:$(N):Jx
    field(DOL1, "$(P)$(R=)WaypointJ:$(N):J1")
    field(LNK1, "$(P)$(R=)Control:J1Cmd PP")
    field(DOL2, "$(P)$(R=)WaypointJ:$(N):J2")
    field(LNK2, "$(P)$(R=)Control:J2Cmd PP")
    field(DOL3, "$(P)$(R=)WaypointJ:$(N):J3")
    field(LNK3, "$(P)$(R=)Control:J3Cmd PP")
    field(DOL4, "$(P)$(R=)WaypointJ:$(N):J4")
    field(LNK4, "$(P)$(R=)Control:J4Cmd PP")
    field(DOL5, "$(P)$(R=)WaypointJ:$(N):J5")
    field(LNK5, "$(P)$(R=)Control:J5Cmd PP")
    field(DOL6, "$(P)$(R=)WaypointJ:$(N):J6")
    field(LNK6, "$(P)$(R=)Control:J6Cmd PP")
    
    field(FLNK, "$(P)$(R=)WaypointJ:$(N):moveJ_seq2.PROC")
}

record(seq, "$(P)$(R=)WaypointJ:$(N):moveJ_seq2")
{
    # Set to asynchronous motion
    field(DOL0, "1")
    field(LNK0, "$(P)$(R=)Control:Asynchronous")
    
    # set speed, accel, blend, gripper action
    field(DOL1, "$(P)$(R=)WaypointJ:$(N):Speed")
    field(LNK1, "$(P)$(R=)Control:JointSpeed PP")
    field(DOL2, "$(P)$(R=)WaypointJ:$(N):Acceleration")
    field(LNK2, "$(P)$(R=)Control:JointAcceleration PP")
    field(DOL3, "$(P)$(R=)WaypointJ:$(N):Blend")
    field(LNK3, "$(P)$(R=)Control:JointBlend PP")
    field(DOL4, "$(P)$(R=)WaypointJ:$(N):Gripper")
    field(LNK4, "$(P)$(R=)Control:WaypointGripperAction PP")
    
    # Do the move
    field(FLNK, "$(P)$(R=)Control:waypoint_moveJ.PROC")
}
